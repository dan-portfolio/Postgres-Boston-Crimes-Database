    "#Clearing privileges from public group\n",
    "conn = psycopg2.connect(dbname=\"crime_db\", user=\"dq\")\n",
    "cur = conn.cursor()\n",
    "\n",
    "cur.execute(\"\"\"\n",
    "    REVOKE ALL ON ALL TABLES IN SCHEMA public FROM public;\n",
    "    REVOKE ALL ON DATABASE crime_db FROM public;\n",
    "\"\"\")\n",
    "\n",
    "conn.commit()\n",
    "conn.close()"
   ]
